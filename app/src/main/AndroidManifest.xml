<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.plent">

    <application
        android:allowBackup="true"
        android:icon="@drawable/plent_logo_v2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeNoActionBar">
        <activity android:name=".myActivities.EventActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".myActivities.FindEventsActivity"
            android:parentActivityName=".myActivities.EventActivity"
            android:label="@string/find_events_activity" >
        </activity>
        <activity android:name=".myActivities.CalendarActivity"
            android:parentActivityName=".myActivities.EventActivity"
            android:label="Calendar" >
        </activity>
    </application>

</manifest>